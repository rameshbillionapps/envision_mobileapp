{
parser: "babel-eslint",
    "env": {
        "browser": true,
        "node": true
    },

    "globals": {
        "React": true
    },
"parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module",
      "ecmaFeatures": {
          "jsx": true,
      }
  },
    "plugins": [
        "react"
    ]
}
